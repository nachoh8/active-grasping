{
    "project": "gramacy-opt",
    "mode": "prod",
    "exp_params": {
        "name": "Gramacy optimization",
        "type": "offline",
        "parallel_bandwidth": 1,
        "budget": 100,
        "metrics": [
            {
                "name":"outcome",
                "strategy":"optimize",
                "objective":"maximize"
            }
        ],
        "parameters": [
            {
                "name": "x1",
                "type":"double",
                "bounds": {"min": 0.0001, "max": 1.0000},
                "prior": {"mean": 0.5, "name": "normal", "scale": 0.2}
            },
            {
                "name": "x2",
                "type":"double",
                "bounds": {"min": 0.0001, "max": 1.0000},
                "prior": {"mean": 0.5, "name": "normal", "scale": 0.2}
            }
        ]
    }
}